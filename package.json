{
  "name": "react-project",
  "version": "0.0.1",
  "description": "The FE project boilerplate based on React",
  "main": "index.js",
  "scripts": {
    "dev-HMR-server": "webpack-dev-server --env=local --open --progress",
    "eslint": "eslint src --ext .jsx,.js --fix",
    "stylelint": "stylelint src/**/*.scss --fix --syntax scss",
    "stylefmt": "stylefmt -c .stylelintrc.js -r src/**/*.scss ",
    "test": "jest --config jest.config.json -i",
    "test-update-snapshot": "jest --config jest.config.json --updateSnapshot -i",
    "build-dev": "webpack --env=dev --progress --profile",
    "build-dev-no-test": "webpack --env=dev --progress --profile",
    "build-prod": "webpack --env=prod --progress --profile",
    "prebuild-dev": "npm run test",
    "prebuild-prod": "npm run test"
  },
  "author": "MLP-CD",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "css-hot-loader": "^1.1.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^2.11.0",
    "postcss-loader": "^2.0.5",
    "pug": "^2.0.0-rc.2",
    "pug-loader": "^2.3.0",
